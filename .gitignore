# ignore all mu-plugins, plugins, and themes
# unless explicitly whitelisted at the end of this file
wp-content/uploads
wp-content/themes/wp-bootstrap-starter-child/assets/generated_files
wp-content/themes/wp-bootstrap-starter-child/node_modules
keys.php

# ignore all files starting with . or ~
~*

# ignore log files and databases
*.log
*.sql
*.sqlite

# ignore compiled files
*.com
*.class
*.dll
*.exe
*.o
*.so

# ignore packaged files
*.7z
*.dmg
*.gz
*.iso
*.jar
*.rar
*.tar
*.zip

